<% if @song.errors.full_messages.length > 0%>
    <% @song.errors.full_messages.each do |error_message| %>
        <%= error_message%>
    <% end %>
<% end %>

<%= form_for @song do |f|%>
<%= f.text_field :title %>
<%= f.text_field :release_year %>
<%= f.text_field :released %>
<%= f.text_field :genre %>
<%= f.text_field :artist_name %>
<% end %> 